{
  "name": "branchy",
  "displayName": "branchy",
  "description": "A VSCode extension that helps you keep track of your working files based on the feature branch you're currently in.",
  "version": "0.1.1",
  "publisher": "gaganganapathyas",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodHeK/branchy"
  },
  "icon": "/assets/marketplace-logo.png",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "branchy",
      "branchy.multipleRepositoriesEnabled": {
        "type": "boolean",
        "default": true,
        "description": "Stores tabs across multiple repositories for a branch. Setting it `false` would only store tabs related to it's repository for a branch."
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.52.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^7.5",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "lodash": "^4.17.21"
  }
}
